{
  "build": {
    "command": "cd .. && pnpm install && pnpm build",
    "env": {
      "PNPM_VERSION": "9.12.3",
      "TURBO_VERSION": "1.13.4"
    }
  },
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
